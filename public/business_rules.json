  "validationChecks": {
    "description": "All automated validation checks performed during KYC enrichment",
    "companiesHouseChecks": [
      {
        "checkCode": "CH_NAME_MATCH",
        "description": "Verify intake legal name matches Companies House registered name",
        "passCondition": "Names match ignoring case, punctuation, and Ltd/Limited variations",
        "failCondition": "Names do not match or company not found",
        "source": "CompanyAnalysisAgent",
        "automated": true
      },
      {
        "checkCode": "CH_FILING_CURRENT",
        "description": "Verify company filings are up to date",
        "passCondition": "Last accounts and confirmation statement both filed within 15 months",
        "failCondition": "Either filing is overdue or older than 15 months",
        "source": "CompanyAnalysisAgent",
        "automated": true
      },
      {
        "checkCode": "CH_ACTIVE",
        "description": "Verify company status is active",
        "passCondition": "Company status is 'active'",
        "failCondition": "Company is dissolved, liquidated, or in administration",
        "source": "CompanyAnalysisAgent",
        "automated": true
      },
      {
        "checkCode": "CH_DIRECTORS_PEP",
        "description": "Screen directors and PSCs for PEP/sanctions risk based on nationality and country of residence",
        "passCondition": "All active directors and PSCs from LOWER risk countries",
        "failCondition": "Any director or PSC from HIGHER risk country or missing nationality data",
        "source": "CompanyAnalysisAgent",
        "automated": true
      },
      {
        "checkCode": "CH_DIRECTORS_EXIST",
        "description": "Verify company has at least one active director",
        "passCondition": "At least 1 active director found",
        "failCondition": "No active directors (possible shell company indicator)",
        "source": "CompanyAnalysisAgent",
        "automated": true
      },
      {
        "checkCode": "CH_ADDRESS_MATCH",
        "description": "Verify registered address country matches intake country",
        "passCondition": "Address country matches intake jurisdiction",
        "failCondition": "Address country does not match or address is missing",
        "source": "CompanyAnalysisAgent",
        "automated": true
      }
    ],
    "fcaChecks": [
      {
        "checkCode": "FCA_REGULATED",
        "description": "Verify firm is authorised or registered with the FCA",
        "passCondition": "Firm status is 'Authorised' or 'Registered'",
        "failCondition": "Firm not found, cancelled, revoked, or not authorised",
        "source": "FCAAnalysisAgent",
        "automated": true
      },
      {
        "checkCode": "FCA_CLIENT_MONEY",
        "description": "Verify firm's client money permission status",
        "passCondition": "Firm has 'Hold and control client money' permission",
        "failCondition": "Firm does not hold client money permission",
        "source": "FCAAnalysisAgent",
        "automated": true,
        "note": "FAIL does not block - informational for risk assessment"
      },
      {
        "checkCode": "FCA_NAME_MATCH",
        "description": "Verify intake name matches FCA registered name",
        "passCondition": "Names match ignoring case and Ltd/Limited variations",
        "failCondition": "Names do not match - possible wrong FCA entity selected",
        "source": "FCAAnalysisAgent",
        "automated": true
      }
    ],
    "riskChecks": [
      {
        "checkCode": "RISK_COUNTRY",
        "description": "Assess country risk level from Territorial Watch List",
        "passCondition": "Country classified as LOWER risk",
        "failCondition": "Country classified as HIGHER risk or not found in list",
        "source": "RulesEngineAgent",
        "automated": true,
        "note": "FAIL does not block - triggers enhanced CDD document set"
      }
    ],

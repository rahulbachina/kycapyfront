  "roleMapping": {
    "description": "Maps intake email roleCategory/clientRoleType to standardised roleMain",
    "mappings": {
      "Client Role Type": {
        "roleMain": "Client",
        "description": "Direct clients including assureds, cedants, and policyholders"
      },
      "Third Party Role Type": {
        "roleMain": "Third Party",
        "description": "Third party intermediaries, TPAs, claims handlers"
      },
      "Underwriter Role Type": {
        "roleMain": "Underwriter",
        "description": "Insurance/reinsurance underwriters, carriers, syndicates"
      }
    },
    "fallbackRules": {
      "description": "When roleCategory and clientRoleType are both null/empty/Unknown",
      "rules": [
        {
          "condition": "SIC codes contain 65120 or 66220 (insurance)",
          "result": "Client",
          "confidence": "MEDIUM"
        },
        {
          "condition": "FCA regulated insurance broker",
          "result": "Client",
          "confidence": "MEDIUM"
        },
        {
          "condition": "Completely unknown",
          "result": "Client",
          "confidence": "LOW",
          "note": "Safest default - flag for manual review"
        }
      ]
    },
